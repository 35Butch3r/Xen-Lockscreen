<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>iOS 10 Camera</string>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
		</dict>
		<dict>
			<key>PostNotification</key>
			<string>com.matchstic.xen/settingschanged</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.matchstic.xen</string>
			<key>key</key>
			<string>iOS10CameraEnabled</string>
			<key>label</key>
			<string>Use iOS 10 Camera Page</string>
		</dict>
     	<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Page Location</string>
			<key>footerText</key>
			<string>The Camera page can only be placed at either of the far ends of the lockscreen.</string>
			<key>segmentHeader</key>
			<true/>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSegmentCell</string>
			<key>validValues</key>
			<array>
				<integer>0</integer>
				<integer>1</integer>
			</array>
			<key>validTitles</key>
			<array>
				<string>Left</string>
				<string>Right</string>
			</array>
			<key>default</key>
			<integer>1</integer>
			<key>defaults</key>
			<string>com.matchstic.xen</string>
			<key>key</key>
			<string>iOS10CameraPosition</string>
			<key>PostNotification</key>
			<string>com.matchstic.xen/settingschanged</string>
		</dict>	
	</array>
</dict>
</plist>
