<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>Morning Summary</string>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Morning Summary will display when your device is idle, and is close to the time when you wake up.</string>
		</dict>
		<dict>
			<key>PostNotification</key>
			<string>com.matchstic.xen/settingschanged</string>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.matchstic.xen</string>
			<key>key</key>
			<string>welcomeController</string>
			<key>label</key>
			<string>Use Morning Summary</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Days where you sleep past the time set are automatically handled, along with awaking within an hour beforehand.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSDefaultCell</string>
			<key>cellClass</key>
			<string>XENPWelcomePickerCell</string>
			<key>height</key>
			<integer>260</integer>
		</dict>
	</array>
</dict>
</plist>
