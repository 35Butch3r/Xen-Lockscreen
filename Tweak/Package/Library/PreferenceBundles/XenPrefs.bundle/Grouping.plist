<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>title</key>
	<string>Grouping</string>
	<key>items</key>
	<array>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key>
			<string>Grouping Style</string>
			<key>footerText</key>
			<string>Grouping brings all notifications from each app together, which are shown after tapping the app's icon on the lockscreen.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSDefaultCell</string>
			<key>cellClass</key>
			<string>XENPGroupedSwitchCell</string>
			<key>height</key>
			<integer>180</integer>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Automatically open the Notification Group for a new notification if no groups are currently expanded, or if no notifications are shown.</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSwitchCell</string>
			<key>label</key>
			<string>Auto Expand Notifications</string>
			<key>default</key>
			<true/>
			<key>defaults</key>
			<string>com.matchstic.xen</string>
			<key>key</key>
			<string>autoExpandNotifications</string>
			<key>PostNotification</key>
			<string>com.matchstic.xen/settingschanged</string>
		</dict>
	</array>
</dict>
</plist>
